{
  "name": "@eevee/eevee-repo",
  "version": "1.0.0",
  "description": "My blog call eevee",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lexuantien/eevee"
  },
  "license": "MIT",
  "workspaces": {
    "packages": [
      "app/*",
      "packages/*",
      "packages/eevee/*",
      "packages/eevee-components/*",
      "scripts",
      "typings"
    ],
    "nohoist": []
  },
  "scripts": {
    "change": "beachball change --no-commit",
    "check:change": "beachball check",
    "check:modified-files": "yarn workspace @eevee/scripts just check-for-modified-files",
    "clean": "lage clean --verbose",
    "code-style": "lage code-style --verbose",
    "format": "node scripts/format.js",
    "lint": "lage lint --verbose",
    "lint:log": "FORCE_COLOR=0 yarn lint > lint.log 2>&1",
    "preinstall": "node ./scripts/use-yarn-please.js",
    "postinstall": "node ./scripts/postinstall.js",
    "publish:beachball": "beachball publish -b origin/master --access public -y",
    "test": "lage test --verbose",
    "create-component": "plop --plopfile ./scripts/create-files/component/plopfile.ts --dest . --require ./scripts/ts-node-register"
  },
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@eevee/eslint-plugin": "*",
    "@griffel/eslint-plugin": "^1.0.0",
    "@griffel/react": "1.3.1",
    "@nrwl/jest": "^14.1.9",
    "@react-spring/web": "^9.4.5",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.6",
    "@storybook/core-server": "^6.5.6",
    "@storybook/react": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.6",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/autoprefixer": "^10.2.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/eslint": "7.2.13",
    "@types/jest": "^27.5.1",
    "@types/jju": "^1.4.1",
    "@types/lerna-alias": "^3.0.1",
    "@types/node": "^17.0.36",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2.6.3",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/semver": "^5.5.0",
    "@types/yargs": "^17.0.10",
    "@types/yargs-unparser": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/experimental-utils": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "beachball": "^2.22.4",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^2.19.0",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "imports-loader": "^4.0.0",
    "jest": "^26.6.3",
    "jju": "^1.4.0",
    "just-scripts": "^2.0.0",
    "lage": "^0.41.0",
    "lerna-alias": "^3.0.2",
    "lint-staged": "10.2.10",
    "lodash": "^4.17.21",
    "postcss-color-rebeccapurple": "^7.0.2",
    "react-router-dom": "6.4.0-pre.15",
    "react-test-renderer": "^18.1.0",
    "semver": "^6.2.0",
    "syncpack": "5.6.10",
    "tmp": "^0.2.1",
    "ts-jest": "26.5.6",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "3.9.0",
    "tslib": "2.4.0",
    "typescript": "4.8.3",
    "yargs": "^17.5.1",
    "yargs-parser": "13.1.2",
    "yargs-unparser": "^2.0.0",
    "plop": "^2.6.0",
    "node-plop": "^0.25.0",
    "@types/scheduler": "0.16.2",
    "octokit": "2.0.7",
    "path-browserify": "1.0.1"
  },
  "syncpack": {
    "prod": true,
    "dev": true,
    "peer": false,
    "source": [
      "package.json",
      "packages/eevee/*/package.json",
      "packages/eevee-components/*/package.json",
      "scripts/package.json"
    ],
    "versionGroups": [
      {
        "packages": [
          "@eevee/eevee-repo"
        ],
        "dependencies": [
          "@babel/core",
          "@babel/preset-typescript",
          "@rnx-kit/eslint-plugin",
          "@types/react-test-renderer",
          "@typescript-eslint/parser",
          "@typescript-eslint/experimental-utils",
          "@typescript-eslint/eslint-plugin",
          "chalk",
          "eslint-config-airbnb",
          "eslint-config-prettier",
          "eslint-import-resolver-typescript",
          "eslint-plugin-deprecation",
          "eslint-plugin-import",
          "eslint-plugin-jest",
          "eslint-plugin-jsx-a11y",
          "eslint-plugin-react",
          "eslint-plugin-react-hooks",
          "fs-extra",
          "glob",
          "jju",
          "webpack",
          "yargs",
          "tslib"
        ]
      }
    ]
  }
}
